{
  "name": "checklist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "prebuild": "vitest",
    "deploy": "vite build && vite-plugin-gh-pages",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:dev": "start-server-and-test dev http://localhost:4000 test:e2e:headed"
  },
  "dependencies": {
    "@playwright/test": "1.47.2",
    "@testing-library/jest-dom": "6.5.0",
    "jsdom": "25.0.1",
    "playwright": "1.47.2",
    "vitest": "2.1.1",
    "vue": "3.4.31",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@testing-library/vue": "8.1.0",
    "@types/jest": "29.5.13",
    "@types/testing-library__vue": "5.3.0",
    "@vitejs/plugin-vue": "5.1.4",
    "dotenv": "16.4.5",
    "start-server-and-test": "2.0.8",
    "typescript": "5.2.2",
    "vite": "5.3.4",
    "vite-plugin-gh-pages": "1.0.0",
    "vite-plugin-pwa": "0.20.5",
    "vue-tsc": "2.0.24"
  }
}
